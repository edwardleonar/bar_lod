PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX ex: <https://example.org/>
PREFIX gn: <http://www.geonames.org/ontology#>
PREFIX gont: <https://gont.ch/>
PREFIX purl: <http://purl.org/dc/terms/>
PREFIX schema: <https://schema.org/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<#Agglomeration_mapping>
	rr:logicalTable [ rr:tableName "agglomeration_list.csv" ];
	
	rr:subjectMap [
		rr:template "http://classifications.data.admin.ch/agglomeration/{identifier}";
		rr:class ex:Agglomeration ;
	];
	
	rr:predicateObjectMap [
		rr:predicate purl:identifier ;
		rr:objectMap [
			rr:column "identifier" ;
			rr:datatype xsd:int ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate schema:name ;
		rr:objectMap [
			rr:column "Agglo_name" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:totalNumberMunicipalities ;
		rr:objectMap [
			rr:column "total_nr_gmd" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:numberMunicipalitiesCH ;
		rr:objectMap [
			rr:column "nr_CH_gmd" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:numberOtherMunicipalities ;
		rr:objectMap [
			rr:column "nr_non_CH_gmd" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate gn:population ;
		rr:objectMap [
			rr:column "total_population_2012" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:cHPopulationSize ;
		rr:objectMap [
			rr:column "population_CH" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ex:otherPopulationSize ;
		rr:objectMap [
			rr:column "population_non_CH" ;
		];
	]
.
<#gemeinde_mapping>
	rr:logicalTable [ rr:tableName "gemeinde_agglo_list.csv" ];
	
	rr:subjectMap [
		rr:template "http://classifications.data.admin.ch/municipality/{gmd_identifier}";
		rr:class gont:Municipality ;
	];
	
	rr:predicateObjectMap [
		rr:predicate schema:isPartOf ;
		rr:objectMap [
			rr:template "http://classifications.data.admin.ch/agglomeration/{agglo_identifier}" ;
		];
	]
.