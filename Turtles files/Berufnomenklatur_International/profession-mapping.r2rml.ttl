PREFIX rr: <http://www.w3.org/ns/r2rml#>
PREFIX purl: <http://purl.org/dc/terms/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<#Berufsabteilungen_map>
	rr:logicalTable [ rr:tableName "labels_data.csv" ];
	
	rr:subjectMap [
		rr:template "http://http://classifications.data.admin.ch/berufsabteilungen_int/{Berufsabteilungen}";
		rr:class skos:Concept ;
	];
	
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rr:column "name_occup_de" ;
			rr:language "de" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rr:column "name_occup_fr" ;
			rr:language "fr" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rr:column "name_occup_it" ;
			rr:language "it" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rr:column "name_occup_en" ;
			rr:language "en" ;
		];
	];
	rr:predicateObjectMap [
		rr:predicate purl:identifier ;
		rr:objectMap [
			rr:column "Berufsabteilungen" ;
			rr:datatype xsd:int ;
		];
	]
.
